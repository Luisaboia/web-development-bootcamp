<%- include('header') -%>
      <div class="box" id="heading">
      <!-- kindOfDay will be replaced by our 'day' variable on app.js -->
      <h1><%=listTitle%></h1>
    </div>
    <div class="box">
    <!-- We use a for loop to add more things on our list -->
    <% for(let i=0;i<newItems.length; i++){ %>
      <div class="item">
        <input type="checkbox">
        <p><%=newItems[i]%></p>
      </div>
    <% } %>
      <form class="item" action="/" method="POST">
        <input type="text" name="newItem" placeholder="New Item" autocomplete="off" />
        <button type="submit" name="list" value="<%=listTitle%>">+</button>
      </form>
    </div>
<%- include('footer') -%>